FoamFile
{
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

vertices
(
    (0 0 0)
    (4 0 0)
    (4 1 0)
    (1 1 0)
    (1 4 0)
    (0 4 0)
    (0 0 3)
    (4 0 3)
    (4 1 3)
    (1 1 3)
    (1 4 3)
    (0 4 3)
);

blocks
(
    hex (0 1 2 3 6 7 8 9) (10 1 1) simpleGrading (1 1 1)
    hex (0 3 4 5 6 9 10 11) (1 10 1) simpleGrading (1 1 1)
);

boundary
(
    end1
    {
        type patch;
        faces
        (
            (4 5 11 10)
        );
    }
    end2
    {
        type patch;
        faces
        (
            (1 7 8 2)
        );
    }
    insulated
    {
        type patch;
        faces
        (
            (1 7 6 0)
            (2 3 9 8)
            (3 4 10 9)
            (0 6 11 5)
        );
    }
    top_and_bottom
    {
        type empty;
        faces
        (
            (0 1 2 3)
            (0 3 4 5)
            (6 7 8 9)
            (6 9 10 11)
        );
    }
);


