FoamFile
{
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

vertices
(
    name p0 (0 0 0)
    name p1 (4 0 0)
    name p2 (4 1 0)
    name p3 (1 1 0)
    name p4 (1 4 0)
    name p5 (0 4 0)
    name p6 (0 0 3)
    name p7 (4 0 3)
    name p8 (4 1 3)
    name p9 (1 1 3)
    name p10 (1 4 3)
    name p11 (0 4 3)
);

blocks
(
    hex (p0 p1 p2 p3 p6 p7 p8 p9) (10 1 1) simpleGrading (1 1 1)
    hex (p0 p3 p4 p5 p6 p9 p10 p11) (1 10 1) simpleGrading (1 1 1)
);

boundary
(
    end1
    {
        type patch;
        faces
        (
            (p4 p5 p11 p10)
        );
    }
    end2
    {
        type patch;
        faces
        (
            (p1 p7 p8 p2)
        );
    }
    insulated
    {
        type patch;
        faces
        (
            (p1 p7 p6 p0)
            (p2 p3 p9 p8)
            (p3 p4 p10 p9)
            (p0 p6 p11 p5)
        );
    }
    top_and_bottom
    {
        type empty;
        faces
        (
            (p0 p1 p2 p3)
            (p0 p3 p4 p5)
            (p6 p7 p8 p9)
            (p6 p9 p10 p11)
        );
    }
);


